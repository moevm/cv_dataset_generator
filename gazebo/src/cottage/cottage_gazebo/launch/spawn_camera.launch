<launch>
  <!-- Convert xacro and put on parameter server -->
  <param name="robot_description" command="$(find xacro)/xacro $(find cottage_gazebo)/urdf/camera.urdf.xacro" />

  <!-- Spawn a robot into Gazebo -->
  <node name="spawn_camera" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -model camera" />
</launch>
